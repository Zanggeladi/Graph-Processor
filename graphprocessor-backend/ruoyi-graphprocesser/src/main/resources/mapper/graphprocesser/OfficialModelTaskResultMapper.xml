<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.graphprocesser.mapper.OfficialModelTaskResultMapper">
    
    <resultMap type="OfficialModelTaskResult" id="OfficialModelTaskResultResult">
        <result property="id"    column="id"    />
        <result property="taskId"    column="task_id"    />
        <result property="outputMessage"    column="output_message"    />
        <result property="resultFilePath"    column="result_file_path"    />
        <result property="curvePath"    column="curve_path"    />
        <result property="tsnePath"    column="tsne_path"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectOfficialModelTaskResultVo">
        select id, task_id, output_message, result_file_path, curve_path, tsne_path, create_by, create_time, update_by, update_time, remark from tb_official_model_task_result
    </sql>

    <select id="selectOfficialModelTaskResultList" parameterType="OfficialModelTaskResult" resultMap="OfficialModelTaskResultResult">
        <include refid="selectOfficialModelTaskResultVo"/>
        <where>  
            <if test="taskId != null "> and task_id = #{taskId}</if>
            <if test="outputMessage != null  and outputMessage != ''"> and output_message like concat('%', #{outputMessage}, '%')</if>
        </where>
    </select>
    
    <select id="selectOfficialModelTaskResultById" parameterType="Long" resultMap="OfficialModelTaskResultResult">
        <include refid="selectOfficialModelTaskResultVo"/>
        where id = #{id}
    </select>

    <select id="selectOfficialModelTaskResultByTaskId" parameterType="Long" resultMap="OfficialModelTaskResultResult">
        <include refid="selectOfficialModelTaskResultVo"/>
        where task_id = #{taskId}
    </select>

    <insert id="insertOfficialModelTaskResult" parameterType="OfficialModelTaskResult" useGeneratedKeys="true" keyProperty="id">
        insert into tb_official_model_task_result
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="taskId != null">task_id,</if>
            <if test="outputMessage != null">output_message,</if>
            <if test="resultFilePath != null">result_file_path,</if>
            <if test="curvePath != null">curve_path,</if>
            <if test="tsnePath != null">tsne_path,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="taskId != null">#{taskId},</if>
            <if test="outputMessage != null">#{outputMessage},</if>
            <if test="resultFilePath != null">#{resultFilePath},</if>
            <if test="curvePath != null">#{curvePath},</if>
            <if test="tsnePath != null">#{tsnePath},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateOfficialModelTaskResult" parameterType="OfficialModelTaskResult">
        update tb_official_model_task_result
        <trim prefix="SET" suffixOverrides=",">
            <if test="taskId != null">task_id = #{taskId},</if>
            <if test="outputMessage != null">output_message = #{outputMessage},</if>
            <if test="resultFilePath != null">result_file_path = #{resultFilePath},</if>
            <if test="curvePath != null">curve_path = #{curvePath},</if>
            <if test="tsnePath != null">tsne_path = #{tsnePath},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteOfficialModelTaskResultById" parameterType="Long">
        delete from tb_official_model_task_result where id = #{id}
    </delete>

    <delete id="deleteOfficialModelTaskResultByIds" parameterType="String">
        delete from tb_official_model_task_result where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>