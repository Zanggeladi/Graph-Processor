<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.graphprocesser.mapper.ExperimentTaskMapper">
    
    <resultMap type="ExperimentTask" id="ExperimentTaskResult">
        <result property="id"    column="id"    />
        <result property="taskName"    column="task_name"    />
        <result property="description"    column="description"    />
        <result property="themeId"    column="theme_id"    />
        <result property="datasetId"    column="dataset_id"    />
        <result property="modeCode"    column="mode_code"    />
        <result property="hyperparameters"    column="hyperparameters"    />
        <result property="status"    column="status"    />
        <result property="execMessage"    column="exec_message"    />
        <result property="userId"    column="user_id"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <resultMap id="ExperimentTaskTbExperimentResultResult" type="ExperimentTask" extends="ExperimentTaskResult">
        <collection property="tbExperimentResultList" ofType="TbExperimentResult" column="id" select="selectTbExperimentResultList" />
    </resultMap>

    <resultMap type="TbExperimentResult" id="TbExperimentResultResult">
        <result property="id"    column="id"    />
        <result property="taskId"    column="task_id"    />
        <result property="metrics"    column="metrics"    />
        <result property="resultFilePath"    column="result_file_path"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectExperimentTaskVo">
        select id, task_name, description, theme_id, dataset_id, mode_code, hyperparameters, status, exec_message, user_id, create_by, create_time, update_by, update_time, remark from tb_experiment_task
    </sql>

    <select id="selectExperimentTaskList" parameterType="ExperimentTask" resultMap="ExperimentTaskResult">
        <include refid="selectExperimentTaskVo"/>
        <where>  
            <if test="taskName != null  and taskName != ''"> and task_name like concat('%', #{taskName}, '%')</if>
            <if test="themeId != null "> and theme_id = #{themeId}</if>
            <if test="datasetId != null "> and dataset_id = #{datasetId}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
        </where>
    </select>
    
    <select id="selectExperimentTaskById" parameterType="Long" resultMap="ExperimentTaskTbExperimentResultResult">
        select id, task_name, description, theme_id, dataset_id, mode_code, hyperparameters, status, exec_message, user_id, create_by, create_time, update_by, update_time, remark
        from tb_experiment_task
        where id = #{id}
    </select>

    <select id="selectTbExperimentResultList" resultMap="TbExperimentResultResult">
        select id, task_id, metrics, result_file_path, create_by, create_time, update_by, update_time, remark
        from tb_experiment_result
        where task_id = #{task_id}
    </select>

    <insert id="insertExperimentTask" parameterType="ExperimentTask" useGeneratedKeys="true" keyProperty="id">
        insert into tb_experiment_task
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="taskName != null and taskName != ''">task_name,</if>
            <if test="description != null">description,</if>
            <if test="themeId != null">theme_id,</if>
            <if test="datasetId != null">dataset_id,</if>
            <if test="modeCode != null and modeCode != ''">mode_code,</if>
            <if test="hyperparameters != null">hyperparameters,</if>
            <if test="status != null">status,</if>
            <if test="execMessage != null">exec_message,</if>
            <if test="userId != null">user_id,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="taskName != null and taskName != ''">#{taskName},</if>
            <if test="description != null">#{description},</if>
            <if test="themeId != null">#{themeId},</if>
            <if test="datasetId != null">#{datasetId},</if>
            <if test="modeCode != null and modeCode != ''">#{modeCode},</if>
            <if test="hyperparameters != null">#{hyperparameters},</if>
            <if test="status != null">#{status},</if>
            <if test="execMessage != null">#{execMessage},</if>
            <if test="userId != null">#{userId},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateExperimentTask" parameterType="ExperimentTask">
        update tb_experiment_task
        <trim prefix="SET" suffixOverrides=",">
            <if test="taskName != null and taskName != ''">task_name = #{taskName},</if>
            <if test="description != null">description = #{description},</if>
            <if test="themeId != null">theme_id = #{themeId},</if>
            <if test="datasetId != null">dataset_id = #{datasetId},</if>
            <if test="modeCode != null and modeCode != ''">mode_code = #{modeCode},</if>
            <if test="hyperparameters != null">hyperparameters = #{hyperparameters},</if>
            <if test="status != null">status = #{status},</if>
            <if test="execMessage != null">exec_message = #{execMessage},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteExperimentTaskById" parameterType="Long">
        delete from tb_experiment_task where id = #{id}
    </delete>

    <delete id="deleteExperimentTaskByIds" parameterType="String">
        delete from tb_experiment_task where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
    <delete id="deleteTbExperimentResultByTaskIds" parameterType="String">
        delete from tb_experiment_result where task_id in 
        <foreach item="taskId" collection="array" open="(" separator="," close=")">
            #{taskId}
        </foreach>
    </delete>

    <delete id="deleteTbExperimentResultByTaskId" parameterType="Long">
        delete from tb_experiment_result where task_id = #{taskId}
    </delete>

    <insert id="batchTbExperimentResult">
        insert into tb_experiment_result( id, task_id, metrics, result_file_path, create_by, create_time, update_by, update_time, remark) values
        <foreach item="item" index="index" collection="list" separator=",">
            ( #{item.id}, #{item.taskId}, #{item.metrics}, #{item.resultFilePath}, #{item.createBy}, #{item.createTime}, #{item.updateBy}, #{item.updateTime}, #{item.remark})
        </foreach>
    </insert>
</mapper>