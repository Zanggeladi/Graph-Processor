<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.graphprocesser.mapper.ReviewDatasetMapper">
    
    <resultMap type="ReviewDataset" id="ReviewDatasetResult">
        <result property="datasetId"    column="dataset_id"    />
        <result property="datasetName"    column="dataset_name"    />
        <result property="description"    column="description"    />
        <result property="tags"    column="tags"    />
        <result property="datasetPath"    column="dataset_path"    />
        <result property="size"    column="size"    />
        <result property="version"    column="version"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectReviewDatasetVo">
        select dataset_id, dataset_name, description, tags, dataset_path, size, version, status, create_by, create_time, update_by, update_time, remark from dataset
    </sql>

    <select id="selectReviewDatasetList" parameterType="ReviewDataset" resultMap="ReviewDatasetResult">
        <include refid="selectReviewDatasetVo"/>
        <where>  
            <if test="datasetId != null "> and dataset_id = #{datasetId}</if>
            <if test="datasetName != null  and datasetName != ''"> and dataset_name like concat('%', #{datasetName}, '%')</if>
            <if test="description != null  and description != ''"> and description like concat('%', #{description}, '%')</if>
            <if test="tags != null  and tags != ''"> and tags like concat('%', #{tags}, '%')</if>
            <if test="datasetPath != null  and datasetPath != ''"> and dataset_path like concat('%', #{datasetPath}, '%')</if>
            <if test="size != null "> and size = #{size}</if>
            <if test="version != null "> and version = #{version}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectReviewDatasetByDatasetId" parameterType="Long" resultMap="ReviewDatasetResult">
        <include refid="selectReviewDatasetVo"/>
        where dataset_id = #{datasetId}
    </select>

    <insert id="insertReviewDataset" parameterType="ReviewDataset" useGeneratedKeys="true" keyProperty="datasetId">
        insert into dataset
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="datasetName != null and datasetName != ''">dataset_name,</if>
            <if test="description != null and description != ''">description,</if>
            <if test="tags != null and tags != ''">tags,</if>
            <if test="datasetPath != null and datasetPath != ''">dataset_path,</if>
            <if test="size != null">size,</if>
            <if test="version != null">version,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="datasetName != null and datasetName != ''">#{datasetName},</if>
            <if test="description != null and description != ''">#{description},</if>
            <if test="tags != null and tags != ''">#{tags},</if>
            <if test="datasetPath != null and datasetPath != ''">#{datasetPath},</if>
            <if test="size != null">#{size},</if>
            <if test="version != null">#{version},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateReviewDataset" parameterType="ReviewDataset">
        update dataset
        <trim prefix="SET" suffixOverrides=",">
            <if test="datasetName != null and datasetName != ''">dataset_name = #{datasetName},</if>
            <if test="description != null and description != ''">description = #{description},</if>
            <if test="tags != null and tags != ''">tags = #{tags},</if>
            <if test="datasetPath != null and datasetPath != ''">dataset_path = #{datasetPath},</if>
            <if test="size != null">size = #{size},</if>
            <if test="version != null">version = #{version},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where dataset_id = #{datasetId}
    </update>

    <delete id="deleteReviewDatasetByDatasetId" parameterType="Long">
        delete from dataset where dataset_id = #{datasetId}
    </delete>

    <delete id="deleteReviewDatasetByDatasetIds" parameterType="String">
        delete from dataset where dataset_id in 
        <foreach item="datasetId" collection="array" open="(" separator="," close=")">
            #{datasetId}
        </foreach>
    </delete>
</mapper>